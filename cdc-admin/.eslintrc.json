{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "plugins": [
        "react"
    ],    
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }                
    },
    "rules": {
        "no-undef": 1,
        "no-unused-vars": 1         
    }
}